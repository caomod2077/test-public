-- H√†m x·ª≠ l√Ω khi nh·∫•n n√∫t Equip
EquipButton.MouseButton1Click:Connect(function()
    -- L·∫•y t√™n gƒÉng tay t·ª´ TextBox
    local gloveName = TextBox.Text

    -- Ki·ªÉm tra n·∫øu ng∆∞·ªùi ch∆°i ƒë√£ nh·∫≠p t√™n gƒÉng tay
    if gloveName and gloveName ~= "" then
        -- L·∫∑p qua c√°c RemoteEvent trong ReplicatedStorage v√† g·ª≠i t√™n gƒÉng tay
        for i, v in pairs(game.ReplicatedStorage._NETWORK:GetChildren()) do
            if v.ClassName == "RemoteEvent" then
                v:FireServer(gloveName) -- G·ª≠i t√™n gƒÉng tay t·ª´ TextBox
            end
        end
    else
        -- C·∫£nh b√°o n·∫øu ch∆∞a nh·∫≠p t√™n
        warn("Vui l√≤ng nh·∫≠p t√™n gƒÉng tay!")
    end
end)
