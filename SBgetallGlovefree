-- T·∫°o giao di·ªán Frame
Frame.Parent = ScreenGui
Frame.Size = UDim2.new(0.12, 0, 0.12, 0)
Frame.Position = UDim2.new(0.5, -Frame.Size.X.Offset / 2, 0.5, -Frame.Size.Y.Offset / 2)
